<authorization>
  <view class="room">
    <van-cell-group border="{{false}}">
      <van-cell
        wx:for="{{rooms}}"
        wx:key="index"
        title="{{point}}{{building}} {{item}}"
        link-type="navigateTo"
        url="/house_pkg/pages/form/index?point={{point}}&building={{building}}&item={{item}}"
        is-link
      />
    </van-cell-group>
  </view>
</authorization>
